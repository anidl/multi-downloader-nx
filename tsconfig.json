{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"outDir": "./lib",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"downlevelIteration": true,
		"removeComments": true,
		"jsx": "react"
	},
	"exclude": ["./videos", "./tsc.ts", "lib/**/*", "gui/react/**/*"]
}
