{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./lib",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "jsx": "react"
  },
  "exclude": [
    "./videos",
    "./tsc.ts",
    "lib/**/*",
    "gui/react/**/*"
  ]
}